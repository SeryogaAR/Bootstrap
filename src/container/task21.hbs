<div>
  <br>
  {{#with heading}}
  <div class="card">
    <div class="card-body">
      <br>
      {{#with main}}
      <div class="card">
        <div class="card-body">
          <h1>{{title}}</h1>
          <p>{{text}}</p>
          {{#with button_text}}
          <button class="btn btn-primary">{{solid}}</button><br><br>
          <button class="btn btn-outline-primary">{{outline}}</button>
          {{/with}}
        </div>
      </div>
      {{/with}}
      <br>
      {{#with second}}
      <div class="card">
        <div class="card-body">
          <h2>{{title}}</h2>
          <p>{{text}}</p>
          {{#with button_text}}
          <button class="btn btn-secondary">{{solid}}</button><br><br>
          <button class="btn btn-outline-secondary">{{outline}}</button>
          {{/with}}
        </div>
      </div>
      {{/with}}
      <br>
      {{#with subblock}}
      <div class="card">
        <div class="card-body">
          <h2>{{title}}</h2>
          <p>{{text}}</p>
          {{#with button_text}}
          <button class="btn btn-dark">{{solid}}</button><br><br>
          <button class="btn btn-outline-dark">{{outline}}</button>
          {{/with}}
        </div>
      </div>
      {{/with}}
      <br>
    </div>
  </div>
  {{/with}}
  <br>
  {{#with nested_display}}
  <div class="display-1">{{title}}</div>
  {{#with children_display}}
  <div class="card">
    <div class="card-body">
      <div class="display-4">{{title}}</div>
      <p>{{text}}</p>
      {{#with button}}
      <button class="btn btn-outline-warning">{{text}}</button>
      {{/with}}
    </div>
  </div>
  {{/with}}
  <h6>{{header_text}}<h6>
      {{#with button}}
      <button class="btn btn-info">{{text}}</button>
      {{/with}}
      {{/with}}
      <br>
      {{#with paragraph}}
      <div class="card">
        <div class="card-body">
          <p class="text-uppercase">{{title}}</p>
          <p>{{text}}</p>
          {{#with button}}
          <a href="{{url}}" class="btn btn-link">{{text}}</a>
          {{/with}}
          <div class="card">
            <div class="card-body">
              {{#each list_name}}
              <div>
                <ul>
                  <li>
                    <a href="{{url}}" class="btn btn-link">{{text}}</a>
                  </li>
                </ul>
              </div>
              {{/each}}
            </div>
          </div>
        </div>
      </div>
      {{/with}}

</div>