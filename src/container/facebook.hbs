<div>
  <h2>{{name}} </h2>
  User: {{#each users}}
  <ul>
    <li>
      {{id}}<br>
      {{name}}<br>
      {{gender}}<br>
      {{age}}<br>
      <hr>
      Friends:{{#each friends}} <ul>
        <li>
          {{> id_name}}
          <hr>
          mutual_friends:{{#each mutual_friends}} <ul>
            <li>
              {{> id_name}}
            </li>
          </ul>
          {{/each}}
          <hr>
          {{> groups}}
        </li>
      </ul>
      {{/each}}
      <hr>
      {{> groups}}
    </li>
  </ul>
  {{/each}}

</div>