<main>
  <div>
    {{#with navigation}}
    <div class="row">

      {{#each links}}
      <div class="col-2">
        <a href='{{href}}' class="btn btn-link">{{text}}</a>
      </div>
      {{/each}}

      {{#with button}}
      <div class="col-2">
        <a href='{{href}}' class="btn btn-primary">{{text}}</a>
      </div>
      {{/with}}
    </div>
    {{/with}}

    <hr>

    {{#with header}}

    <h1>{{title}}</h1>
    <p>{{description}}</p>
    {{#with button}}
    <a href="{{link}}" class="btn btn-primary">{{text}}</a>
    {{/with}}

    {{/with}}

    <hr>

    {{#with city}}
    <h2>{{title}}</h2>
    <div class="row g-3">
      {{#each cards}}

      <div class="col-5">
        {{#with picture}}
        <img class="card-img-top" src="{{src}}" alt="{{alt}}">
        {{/with}}
        <div class="card">
          <div class="card-body">
            <div class="card-title">{{title}}</div>
            <p>{{description}}</p>
            {{#with button}}
            <a class="btn btn-primary" href="{{href}}">{{text}}</a>
            {{/with}}
          </div>
        </div>
      </div>
      {{/each}}
    </div>
    {{/with}}

    <hr>

    {{#with winner}}
    <h2>{{title}}</h2>
    {{#with table}}
    <table class="table">
      <thead>
        <tr>
          {{#each head}}
          <th>{{this}}</th>
          {{/each}}
        </tr>

      </thead>
      {{#each user}}
      <tbody>
        <tr>
          <td>{{place}}</td>
          <td>{{name}}</td>
          <td>{{surname}}</td>
          <td>{{nickname}}</td>
        </tr>

      </tbody>
      {{/each}}
    </table>
    {{/with}}
    {{/with}}

    <hr>

    {{#with gallery}}
    <h2>{{title}}</h2>
    <div class="row">
      {{#each cards}}
      <div class="col-4">
        <div class="card">

          <div class="card-header">{{name}}</div>

          <ul class="list-group list-group-flush">
            {{#each items}}
            <li class="list-group-item">{{this}}</li>
            {{/each}}
          </ul>

        </div>
      </div>
      {{/each}}
    </div>
    {{/with}}

    <hr>

    {{#with relaxion}}
    <h2 class="display-3">{{title}}</h2>
    <div class="row g-3">
      {{#each cards}}
      <div class="col-5">
        <div class="card text-bg-secondary">
          <div class="card-header">{{header}}</div>
          <div class="card-body">
            <h2 class="card-title">{{title}}</h2>
            <p>{{description}}</p>
          </div>

        </div>

      </div>
      {{/each}}

    </div>
    {{/with}}

    <hr>
    {{#with rest}}
    <div class="row">
      {{#each cards}}
      <div class="col-6">
        <div class="card">
          <div class="card-header text-center">{{header}}</div>
          <div class="card-body text-center">
            <div class="card-title">{{title}}</div>
            <p>{{description}}</p>
            <button class="btn btn-primary">{{button}}</button>

          </div>
          <div class="card-footer text-center">{{footer}}</div>
        </div>

      </div>
      {{/each}}
    </div>
    {{/with}}

    <hr>
    {{#with proposition}}
    <p class="display-2">{{title}}</p>
    <div class="row">
      {{#each card}}
      <div class="col-5">
        <div class="card text-bg-warning text-light">
          <div class="card-header">{{header}}</div>
          <div class="card-body">
            <h2 class="card-title">{{title}}</h2>
            <p>{{description}}</p>

          </div>

        </div>
      </div>
      {{/each}}

    </div>
    {{/with}}

    <hr>
    {{#with footer}}
    <h5>{{title}}</h5>

    <div class="row">

      <section class="col-3">
        <div class="row g-2">
          {{#each navigation}}
          <a href='{{href}}' class="btn btn-link">{{text}}</a>
          {{/each}}
        </div>
      </section>

      <section class="col-3">
        <div class="row g-2">
          {{#each socialMedia}}
          <a href='{{link}}' class="btn btn-outline-dark">{{name}}</a>
          {{/each}}
        </div>
      </section>

      <section class="col-3">
        <div class="row g-2">
          {{#each partners}}
          <a href='{{link}}' class="btn btn-outline-info">{{name}}</a>
          {{/each}}
        </div>
      </section>

    </div>

    {{/with}}
  </div>
</main>