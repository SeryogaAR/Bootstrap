<div>
  <div>
    {{#with page_header}}
    <h1 class="text-center">{{title}}</h1>
    <p class="text-center">{{description}}</p>
    {{/with}}
  </div>
  {{#with section}}
  <div class="card">
    <div class="card-body">
      {{#with about_us}}
      {{#with title_list}}
      <p class="display-6 text-end">{{main}}</p>
      <h2 class="text-end">{{sub}}</h2>
      {{/with}}
      {{#with button}}
      <button class="btn btn-outline-info">{{text}}</button>
      {{/with}}
      {{/with}}
    </div>
  </div>
  {{#with advantage}}
  <div class="card">
    <div class="card-body">
      <p class="display-5 text-muted">{{title}}</p>
      {{#with efficiency}}
      <div class="card">
        <div class="card-body">
          <p class="display-4 text-muted">{{title}}</p>
          <h6>{{sub_title}}</h6>
          <p>description</p>
          <button class='btn btn-outline-warning'>{{button}}</button>
        </div>
      </div>
      {{/with}}
      {{#with innovation}}
      <div class="card">
        <div class="card-body">
          <p class="display-4 text-muted text-center">{{title}}</p>
          <h6 class="text-center">{{sub_title}}</h6>
          <p class="text-center">description</p>
          <button class='btn btn-outline-warning'>{{button}}</button>
        </div>
      </div>
      {{/with}}
      {{#with collaboration}}
      <div class="card">
        <div class="card-body">
          <p class="display-4 text-muted text-end">{{title}}</p>
          <h6 class="text-end">{{sub_title}}</h6>
          <p class="text-end">description</p>
          <button class='btn btn-outline-warning'>{{button}}</button>
        </div>
      </div>
      {{/with}}

    </div>
  </div>
  {{/with}}
  {{#with agreement}}
  <div class="card">
    <div class="card-body">
      <p class="display-5">{{title}}</p>
      {{#with info}}
      <div class="card">
        <div class="card-body">
          <h5>{{sub_title}}</h5>
          {{#with detail}}
          <div class="card">
            <div class="card-body">
              <p>{{text}}</p>
              <button class="btn btn-success">{{button}}</button>
            </div>
          </div>
          {{/with}}

        </div>
      </div>
      {{/with}}


    </div>
  </div>

  {{/with}}

  {{/with}}
</div>